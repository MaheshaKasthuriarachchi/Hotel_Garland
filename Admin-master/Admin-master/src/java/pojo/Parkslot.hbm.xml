<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.Parkslot" table="parkslot" catalog="rms1" optimistic-lock="version">
        <id name="slotId" type="java.lang.Integer">
            <column name="slotId" />
            <generator class="identity" />
        </id>
        <property name="available" type="string">
            <column name="available" length="45" not-null="true" />
        </property>
        <property name="slotname" type="string">
            <column name="slotname" length="244" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
